//Task3
import java.util.Scanner;
public class task3
{
  public static void main(String [] args)
  {
    Scanner sc = new Scanner(System.in);
    System.out.print("N = ");
    int N = sc.nextInt();
    int [] arr = new int [N];
    for(int i=0; i<N; i++)
    {
      arr [i] = sc.nextInt(); 
    }
  } 
}